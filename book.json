            "qzone": true, 
            "stumbleupon": false, 
            "twitter": false, 
            "viber": false, 
            "vk": false, 
            "weibo": true, 
            "whatsapp": false, 
            "all": [
                "facebook", 
                "google", 
                "twitter", 
                "weibo", 
                "qq", 
                "linkedin", 
                "qzone", 
                "douban"
            ]
        },
         "edit-link": {
            "base": "https://github.com/curder/laravel_study/edit/master",
