    },
    "links": {
        "sidebar": {
            "Home": "https://github.com/curder"
        }
    },
    "plugins": [
        "-sharing",
        "sharing-plus",
        "-lunr",
        "-search",
        "search-pro",
        "collapsible-menu",
        "edit-link",
        "anchor-navigation-ex@0.1.8",
        "tbfed-pagefooter@^0.0.1"
    ], 
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
